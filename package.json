{
  "name": "jackal-s3-adapter",
  "version": "1.0.0",
  "description": "S3-compatible server that wraps Jackal.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --inspect=4321 src/index.ts"
  },
  "keywords": [
    "jackal",
    "s3",
    "storage",
    "decentralized"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
