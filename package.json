    "@jackallabs/jackal.js": "file:jackallabs-jackal.js-3.6.0.tgz",
    "dotenv": "^16.3.1",
    "fast-xml-parser": "^4.2.5",
    "fastify": "^4.19.2",
    "fastify-plugin": "^5.0.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/ws": "^8.18.0",
    "pino-pretty": "^13.0.0",
    "ts-node-dev": "^2.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.1.6"
  }
}
